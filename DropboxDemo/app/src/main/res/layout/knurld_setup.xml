<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="App-Model"
        android:id="@+id/createAppModel"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="86dp"
        android:onClick="setKnurldAppModel" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Consumer"
        android:id="@+id/createConsumer"
        android:layout_below="@+id/createAppModel"
        android:layout_alignLeft="@+id/createAppModel"
        android:layout_alignStart="@+id/createAppModel"
        android:layout_marginTop="59dp"
        android:onClick="setKnurldConsumer" />
</RelativeLayout>
